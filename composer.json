{
  "name": "blackbit_digital_commerce/pimcore-backup",
  "description": "Backup and restore Pimcore projects",
  "license": "GPL-3.0-or-later",
  "type": "pimcore-bundle",
  "authors": [
    {
      "name": "Blackbit",
      "email": "info@blackbit.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "blackbit\\BackupBundle\\": ""
    }
  },
  "require": {
    "php": ">=7",
    "league/flysystem":"1.*|2.*|3.*",
    "league/flysystem-aws-s3-v3": "*",
    "spatie/flysystem-dropbox": "*"
  },
  "require-dev": {
    "pimcore/pimcore":"5.*|6.*|10.*"
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "blackbit\\BackupBundle\\BackupBundle"
      ]
    }
  }
}
