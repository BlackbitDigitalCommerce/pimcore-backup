{
  "name": "blackbit_digital_commerce/pimcore-backup",
  "description": "Backup and restore Pimcore projects",
  "license": "GPLv3",
  "type": "pimcore-bundle",
  "authors": [
    {
      "name": "Blackbit",
      "email": "info@blackbit.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "blackbit\\BackupBundle\\": ""
    }
  },
  "require": {
    "php": ">=7",
    "league/flysystem":"1.*",
    "league/flysystem-aws-s3-v3": "^1.0",
    "spatie/flysystem-dropbox": "^1.0",
    "league/flysystem-azure-blob-storage": "^0.1.4"
  },
  "require-dev": {
    "pimcore/pimcore":"5.*|6.*"
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "blackbit\\BackupBundle\\BackupBundle"
      ]
    }
  }
}
